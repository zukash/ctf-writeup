def solve(e, n):
    cf = continued_fraction(e / n)
    cand = []
    for conv in cf.convergents():
        k = conv.numerator()
        d = conv.denominator()
        if k == 0:
            continue
        # d * e = k * (n - p - q + 1) + 1
        # p + q = n - (d * e - 1) / k + 1
        pq = n - (d * e - 1) / k + 1
        if not pq.is_integer():
            continue
        # (x - p) * (x - q) = 0 の解が p,q
        # x^2 - (p + q) * x + n = 0 の解が p,q
        cand.append(d)

        # x = var("x")
        # f = x ^ 2 - pq * x + n
        # sol = f.roots()
        # if len(sol) != 2:
        #     continue
        # p, q = sol[0][0], sol[1][0]
        # if not (p.is_integer() and q.is_integer()):
        #     continue
        # if not (is_prime(p) and is_prime(q)):
        #     continue
        # return p, q, d
    return cand


