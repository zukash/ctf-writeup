def has_small_factor(n):
    for p in Primes()[: 10 ** 5]:
        if n % p == 0:
            return True
    return False


def decrypt(c, p, q, e):
    phi = (p - 1) * (q - 1)
    if GCD(e, phi) != 1:
        return b"\x00"
    d = pow(e, -1, phi)
    m = pow(c, d, p * q)
    return long_to_bytes(m)


